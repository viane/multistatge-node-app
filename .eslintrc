{
    "env": {
        "node": true
    },
    "rules": {
        "no-underscore-dangle": 0,
        "quotes": [
            2,
            "single"
        ],
        "consistent-return": 0,
        "no-shadow": 0,
        "camelcase": 0,
        "no-path-concat": 0,
        "no-process-exit": 0,
        "new-cap": 0
    },
    "parserOptions": {
        "ecmaVersion": 7,
        "sourceType": "module",
        "ecmaFeatures": {
            "modules": true
        }
    }
}